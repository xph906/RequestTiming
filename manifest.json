{
  "manifest_version": 2,

  "name": "RequestTiming",
  "description": "get request's timing",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["contentscript.js"],
      "run_at": "document_start"
    }
  ],
  "devtools_page": "devtools.html",
  "permissions": [
    "activeTab",
    "tabs",
    "https://*/*",
	  "http://*/*"
  ]
}
