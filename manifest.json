{
  "manifest_version": 2,

  "name": "RequestTiming",
  "description": "get request's timing",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "devtools_page": "devtools.html",
  "permissions": [
    "activeTab",
    "https://*/*",
	  "http://*/*"
  ]
}
